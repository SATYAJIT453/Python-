class Student:
    def __init__(self,phy,che,math):
        print('BEFORE DELETION')
        print(f'phy is :{phy}')
        print(f'che is :{che}')
        print(f'math is :{math}')
        print('AFTER DELETION')
        del math
        print(f'phy is :{phy}')
        print(f'che is :{che}')
        print(f'math is :{math}')
s1=Student(77,93,75)