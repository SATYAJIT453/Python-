class A:
    def method1(self):
        print('method1')
        
class B(A):
    def method2(self):
        print('method2')
        
class C(A):
    def method3(self):
        print('method3')
        
class D(B,C):
    def method4(self):
        print('methid4')
        
        
s1=A()
s1.method1()
p1=B()
p1.method2()
p1.method1()
m1=C()
m1.method3()
m1.method1()
n1=D()
n1.method1()
n1.method2()
n1.method3()
n1.method4()