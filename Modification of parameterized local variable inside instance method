class Student:
    def check(self,phy,che,math):
        print('BEFORE MODIFICATION')
        print(f'avg is  is :{(phy+che+math)/3}')
        print('AFTER MODIFICATION')
        phy=90
        che=86
        math=88
        print(f'avg is  is :{(phy+che+math)/3}')
        
s1=Student()
s1.check(77,68,90)