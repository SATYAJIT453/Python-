class Student:
    def check(self):
        pass
    @classmethod
    def cm(cls,phy,che,math):
        print('BEFORE DELETION')
        print(f'phy is :{phy}')
        print(f'che is :{che}')
        print(f'math is :{math}')
        print('AFTER DELETION')
        del math
        print(f'phy is :{phy}')
        print(f'che is :{che}')
        print(f'math is :{math}')
s1=Student()
s1.cm(88,84,85)